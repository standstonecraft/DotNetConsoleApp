{
    "[csharp]": {
        "editor.defaultFormatter": "Leopotam.csharpfixformat"
    },
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "csharp.semanticHighlighting.enabled": true,
    "csharp.unitTestDebuggingOptions": {
        "type": "clr",
        "logging": {
            "moduleLoad": true
        }
    },
    "omnisharp.testRunSettings": "DotNetConsoleApp/test/unitTest.runsettings",
    "csharpfixformat.style.newline.atEnd": true,
    "csharpfixformat.style.spaces.beforeIndexerBracket": false,
    "csharpfixformat.style.spaces.beforeParenthesis": false,
    "csharpfixformat.style.spaces.insideEmptyBraces": false,
    "dotnet-test-explorer.testArguments": "-- RunConfiguration.TargetPlatform=x64",
    "dotnet-test-explorer.testProjectPath": "**/*Test.csproj",
    "editor.formatOnSave": true,
    "mssql.connections": [
        {
            "authenticationType": "Integrated",
            "database": "Database1",
            "password": "",
            "profileName": "sqlsrv_Database1",
            "server": "localhost"
        }
    ],
    "mssql.format.datatypeCasing": "lowercase",
    "mssql.format.keywordCasing": "uppercase",
    "mssql.format.placeSelectStatementReferencesOnNewLine": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "sql-formatter.dialect": "pl/sql",
    "sql-formatter.uppercase": true,
    "files.exclude": {
        "*/bin": false,
        "*/obj": false
    },
}